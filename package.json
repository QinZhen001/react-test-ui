{
  "name": "test-aaa-ui-kit",
  "version": "0.1.0",
  "description": "",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/components/index.d.ts",
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "clean": "rimraf ./dist ./lib ./es",
    "storybook": "start-storybook -p 6006",
    "build": "npm run build:storybook",
    "build:storybook": "build-storybook -o dist",
    "build:cjs": "webpack --config ./webpack/webpack.cjs.js",
    "build:es": "webpack --config ./webpack/webpack.es.js",
    "pack": "run-s clean pack:es pack:cjs pack:ts",
    "copy:es": "cp -f ./src/components/index.tsx ./es/index.js",
    "copy:cjs": "cp -f ./src/components/index.tsx ./lib/index.js",
    "pack:ts": "tsc -p tsconfig.json",
    "pack:es": "run-s build:es copy:es",
    "pack:cjs": "run-s build:cjs copy:cjs"
  },
  "dependencies": {
    "@babel/runtime": "7.13.17",
    "classnames": "2.2.6",
    "dayjs": "1.10.4",
    "react-datepicker": "3.6.0"
  },
